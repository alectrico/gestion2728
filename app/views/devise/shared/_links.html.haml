- if controller_name != 'sessions'
  -#= link_to "Log in", new_session_path(resource_name)
  .nav-item.nav-link= link_to('<span class="glyphicon glyphicon-log-in"></span> Iniciar sesión'.html_safe, new_user_session_path)

  %br/
- if devise_mapping.registerable? && controller_name != 'registrations'
  -#link_to "Sign up", new_registration_path(resource_name)
  .nav-item.nav-link= link_to('<span class="glyphicon glyphicon-edit"></span> Registro'.html_safe, new_user_registration_path)


  %br/
- if devise_mapping.recoverable? && controller_name != 'passwords' && controller_name != 'registrations'
  = link_to "¿Olvidó la contraseña?", new_password_path(resource_name)
  %br/
- if devise_mapping.confirmable? && controller_name != 'confirmations'
  = link_to "¿No ha recibido las instrucciones de confirmación?", new_confirmation_path(resource_name)
  %br/
- if devise_mapping.lockable? && resource_class.unlock_strategy_enabled?(:email) && controller_name != 'unlocks'
  = link_to "¿No ha recibido las instrucciones de desloqueo de la cuenta?", new_unlock_path(resource_name)
  %br/
- if devise_mapping.omniauthable?
  - resource_class.omniauth_providers.each do |provider|
    = link_to "Hacer login con #{OmniAuth::Utils.camelize(provider)}", omniauth_authorize_path(resource_name, provider)
    %br/
